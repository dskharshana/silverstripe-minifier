{
	"name": "kavinda/silverstripe-minifier",
	"description": "CSS & JavaScript minifier, in PHP for SilverStripe 4 integrating matthiasmullie/minify",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/dskharshana/silverstripe-minifier",
	"keywords": ["silverstripe", "javascript", "css", "minify"],
	"license": "MIT",
	"authors": [
		{
			"name": "Kavinda Harshana"
		}
	],
	"support": {
		"issues": "https://github.com/axllent/silverstripe-minifier/issues"
	},
    "require": {
		"silverstripe/framework": "5.0.0",
        "matthiasmullie/minify": "^1.3"
	},
    "autoload": {
        "psr-4": {
            "Axllent\\Minifier\\": "src/"
        }
    },
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"silverstripe/vendor-plugin": true
		}
	}
}
